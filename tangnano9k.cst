# Tang Nano 9K Pin Constraints for Stochastic REPL Core

# Clock (27 MHz onboard oscillator)
IO_LOC "clk" 52;
IO_PORT "clk" PULL_MODE=UP;

# Reset button (S1)
IO_LOC "rst" 3;
IO_PORT "rst" PULL_MODE=UP;

# UART (connect to ELM11)
IO_LOC "uart_rx" 17;  # UART receive from ELM11
IO_LOC "uart_tx" 18;  # UART transmit to ELM11
IO_PORT "uart_rx" IO_TYPE=LVCMOS33 PULL_MODE=UP;
IO_PORT "uart_tx" IO_TYPE=LVCMOS33;

# LEDs for status indication (optional, 6 onboard LEDs)
IO_LOC "led[0]" 10;  # Computation active
IO_LOC "led[1]" 11;  # Result ready
IO_LOC "led[2]" 13;  # Error indicator
IO_LOC "led[3]" 14;  # UART activity
IO_LOC "led[4]" 15;  # SNG active
IO_LOC "led[5]" 16;  # Reserved
IO_PORT "led[0]" IO_TYPE=LVCMOS33;
IO_PORT "led[1]" IO_TYPE=LVCMOS33;
IO_PORT "led[2]" IO_TYPE=LVCMOS33;
IO_PORT "led[3]" IO_TYPE=LVCMOS33;
IO_PORT "led[4]" IO_TYPE=LVCMOS33;
IO_PORT "led[5]" IO_TYPE=LVCMOS33;

# SPI Flash (for future persistent storage)
# IO_LOC "flash_clk" 59;
# IO_LOC "flash_cs" 60;
# IO_LOC "flash_miso" 62;
# IO_LOC "flash_mosi" 61;

